{
  "name": "rag-api",
  "homepage": "https://github.com/react-chatbotify/rag-api",
  "version": "0.1.0",
  "description": "A simple RAG API server for answering questions related to React ChatBotify.",
  "scripts": {
    "format": "prettier --write \"{src,test,types}/**/*.{ts,tsx,js,json,md}\"",
    "build": "npx tsc --build",
    "lint": "eslint \"{src,test,types}/**/*.ts\" --fix",
    "test": "jest",
    "start": "docker compose -p rcb-rag-api --env-file ./config/env/.env -f ./docker/docker-compose.yml -f ./docker/docker-compose.override.yml up -d --build",
    "stop": "docker compose -p rcb-rag-api down",
    "start:watch": "nodemon --config nodemon.json"
  },
  "author": "tjtanjin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-chatbotify/rag-api"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.51.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.51.0",
    "@opentelemetry/instrumentation-express": "^0.40.0",
    "@opentelemetry/instrumentation-http": "^0.51.0",
    "@opentelemetry/resources": "^1.24.0",
    "@opentelemetry/sdk-logs": "^0.51.0",
    "@opentelemetry/sdk-node": "^0.51.0",
    "@opentelemetry/semantic-conventions": "^1.24.0",
    "@opentelemetry/winston-transport": "^0.13.0",
    "chromadb-client": "^2.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "marked": "^12.0.2",
    "mongodb": "^6.16.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.14",
    "@types/marked": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  }
}
